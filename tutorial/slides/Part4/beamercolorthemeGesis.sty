\mode<presentation>

\definecolor{hellgrau}{rgb}{0.109375,0.40625, 0.51953125}
\definecolor{dunkelgrau}{rgb}{0.009375,0.30625, 0.41953125}
\definecolor{dunkelgrau2}{rgb}{0.009375,0.20625, 0.31953125}
\definecolor{hellbraun}{rgb}{0.9140625, 0.8984375, 0.8046875}
\definecolor{hellbraun2}{rgb}{.95, .90, .80}
\definecolor{alertred}{rgb}{0.85156250, 0.38281250, 0.08984375}

\setbeamercolor*{normal text}{fg=black,bg=white}
\setbeamercolor*{alerted text}{fg=hellgrau}
\setbeamercolor*{section in toc}{fg=hellgrau}
\setbeamercolor*{subsection in toc}{fg=hellgrau}

\setbeamercolor*{item}{fg=hellgrau}
\setbeamercolor*{subitem}{fg=hellgrau}
\setbeamercolor*{subsubitem}{fg=hellgrau}

\setbeamercolor*{item projected}{parent=item}
\setbeamercolor*{subitem projected}{parent=item}
\setbeamercolor*{subsubitem projected}{parent=subitem}

\setbeamercolor{enumerate item}{parent=item}
\setbeamercolor{enumerate subitem}{parent=item}
\setbeamercolor{enumerate subsubitem}{parent=subitem}

\setbeamercolor{itemize item}{parent=item}
\setbeamercolor{itemize subitem}{parent=item}
\setbeamercolor{itemize subsubitem}{parent=subitem}

\setbeamercolor*{block body}{bg=hellbraun,fg=black}
\setbeamercolor*{block body alerted}{}
\setbeamercolor*{block title}{bg=hellgrau,fg=white}
\setbeamercolor*{block title alerted}{parent=alerted text}

\setbeamercolor*{palette primary}{use=structure,fg=hellgrau,bg=hellbraun}
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=hellgrau}
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg=hellgrau}
\setbeamercolor*{palette quaternary}{fg=white,bg=black}

\setbeamercolor*{sidebar}{use=structure,bg=hellbraun}
  
\setbeamercolor*{palette sidebar primary}{use=structure,fg=black,bg=hellbraun}
\setbeamercolor*{palette sidebar secondary}{fg=black}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=black}
\setbeamercolor*{palette sidebar quaternary}{fg=black}


\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode<all>
